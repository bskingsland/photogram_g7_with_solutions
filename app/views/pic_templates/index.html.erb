<h1>All Photos</h1>

<a href="/photos/new">Add a new photo</a>

<% @list_of_photos.each do |a_photo| %>
<div class="thumbnail">

<img src="<%=a_photo.source%>">

<a class="btn btn-secondary" href="/photos">Go Back</a>
<a class="btn btn-secondary" href="/photos/<%=a_photo.id%>/edit">Edit</a>
<a class="btn btn-secondary" href="/delete_photo/<%=a_photo.id%>">Delete</a>

</div>
<% end %>
